{
  "name": "agio-dao",
  "version": "0.2.0",
  "private": true,
  "homepage": "https://dao.agio.app",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview",
    "lint": "eslint . --ext .ts,.vue,.js --fix",
    "prebuild": "yarn lint",
    "deploy:production": " yarn build && firebase deploy",
    "deploy:staging": "yarn build && netlify deploy --dir=dist",
    "hh:deploy:rinkeby": "npx hardhat run --network rinkeby deploy/deploy.ts",
    "hh:deploy:mumbai": "npx hardhat run --network mumbai deploy/deploy.ts",
    "hh:deploy:local": "npx hardhat run --network local deploy/deploy.ts",
    "hh:node": "npx hardhat node",
    "hh:test": "npx hardhat test",
    "hh:build": "npx hardhat compile",
    "codegen:download": "ts-node scripts/codegen/download-schema",
    "codegen:generate": "ts-node scripts/codegen/generate-types",
    "codegen:clean": "rm -rf ./src/codegen/*",
    "codegen": "yarn codegen:clean && yarn codegen:download && yarn codegen:generate"
  },
  "dependencies": {
    "@apollo/client": "^3.5.10",
